/* pages/index/index.wxss */
.common-page {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    height: 150rpx;
    padding: 0 24rpx;
    background: -webkit-linear-gradient(top,rgb(196, 238, 186),white);
}
text{
  color: rgb(201, 50, 50);
}
.tffzh {
    text-align: center;
    font-size: 50rpx;
    color: #333;
    font-weight: 600;
    margin:auto;
}
.btn_login{
    margin: 30rpx;
    padding: 0rpx;
    line-height: 70rpx;
    font-size: 35rpx;
    width: 100%;
    height: 100%;
    border-radius: 15rpx;
    margin-top: 200rpx;
}
image {
  display: flex;
  justify-content: center;
  border-radius: 30rpx;
  overflow: hidden;
}